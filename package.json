  "license": "MIT",
  "scripts": {
    "db:docker:up": "docker compose -f './db/docker-compose.yml' --env-file ./.env up -d --build",
    "db:docker:down": "docker compose -f './db/docker-compose.yml' --env-file ./.env down",
    "start:zenstack-prisma": "pnpm run zenstack:generate && pnpm run prisma:generate && pnpm run prisma-migrate",
    "start:backend:dev": "pnpm run configproxy && nx serve backend --watch",
    "start:frontend:dev": "pnpm run configangular -- --environment=dev && nx serve frontend --proxy-config ./proxy.conf.json --host 127.0.0.1 --port 4000",
    "start:frontend:script": "dotenv -e ../.env ts-node ./scripts/run-env.js",
    "pnpm-instal": "pnpm install --no-frozen-lockfile",
    "prisma:studio": "dotenv -e ../.env -- pnpm exec prisma studio --schema=./zmodel/prisma/schema.prisma",
    "seed-param": "dotenv -e ../.env ts-node zmodel/prisma/seed-param.ts",
    "seed-faker": "dotenv -e ../.env ts-node zmodel/prisma/seedfaker.ts",
    "seed-org": "dotenv -e ../.env ts-node zmodel/prisma/seedOrg.ts",
    "seed": "pnpm exec prisma db seed",
    "db-reset": "dotenv -e ../.env ts-node zmodel/prisma/resetdb.ts",
    "docker:app:up": "docker compose -f 'compose.yaml' --env-file .env-docker up -d --build",
    "docker:app:down": "docker compose -f 'compose.yaml' --env-file .env-docker down",
    "docker:backend:dev": "pnpm run start:zenstack-prisma && pnpm run configproxy && nx serve backend --watch",
    "docker:frontend:dev": "dotenv -e ../.env-docker ts-node ./scripts/run-env.js",
    "docker:frontend:dev-bis": "pnpm run configangular-docker -- --environment=dev && nx serve frontend --proxy-config ./proxy.conf.json",
    "nx": "pnpm nx",
    "nxupdate": "nx migrate latest",
    "nxmigrate": "nx migrate --run-migrations",
    "pnpm-auto-update": "pnpm update",
    "preinstall": "typesync || :",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "zenstack:generate": "zenstack generate --schema zmodel/schema.zmodel",
    "zenStack:repl": "pnpm exec zenstack repl",
    "zenstack:info": "zenstack info",
    "zenstack:init": "pnpm exec zenstack init -p pnpm",
    "start:prisma": "pnpm run prisma:generate && pnpm run prisma-migrate",
    "generate-metadata-swagger": "ts-node ./apps/backend/src/generate-metadata.ts",
    "prisma:generate": "pnpm exec prisma generate --schema=./zmodel/prisma/schema.prisma",
    "prisma:generate:watch": "pnpm exec prisma generate --schema=./zmodel/prisma/schema.prisma --watch",
    "prisma-migrate": "dotenv -e ../.env -- pnpm exec prisma migrate dev --schema=./zmodel/prisma/schema.prisma",
    "circular-dependency": "pnpm madge --circular apps/backend/src/main.ts",
    "configangular": "dotenv -e ../.env ts-node ./scripts/setenv.ts",
    "configproxy": "dotenv -e ../.env ts-node ./scripts/setproxyconfig.ts",
    "configangular-docker": "dotenv -e ../.env-docker ts-node ./scripts/setenv.ts",
    "configproxy-docker": "dotenv -e ../.env-docker ts-node ./scripts/setproxyconfig.ts",
    "ng": "pnpm nx",
    "postinstall": "node ./decorate-angular-cli.js",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test"
  },
